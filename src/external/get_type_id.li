Header

  + name    := GET_TYPE_ID;

  /*Get intern ID object*/

Inherit

  + parent_expr:Expanded EXPR

Public

  - is_invariant:BOOLEAN <- TRUE

  + receiver:TYPE_FULL

  - static_type:TYPE_FULL <- type_integer.default

  - get_type t:TYPES_TMP <-
  (
    t.add type_integer
  )

  //
  // Creation.
  //

  - create p:POSITION receiver e:TYPE_FULL :SELF <-
  ( + result:SELF

    result := clone
    result.make p receiver e
    result
  )

  - make p:POSITION receiver e:TYPE_FULL <-
  (
    position := p
    receiver := e
  )

  - my_copy:SELF <- SELF.create position receiver receiver
  // BSBS: Tu peux renvoyer Self ! Non ?

  //
  // Remove
  //

  - remove <-
  (
    // Nothing.
  )

  //
  // Execute
  //

  - execute_unlink:INSTR <-
  (
    NULL
  )

  - execute_link:EXPR <-
  (
    Self
  )

  //
  // Genere.
  //

  - genere <-
  (
    (receiver.is_late_binding).if {
      output.append "-1"
    } else {
      output.append "__"
      output.append (receiver.raw.intern_name)
      output.append "__"
    }
  )

  //
  // Display.
  //

  - display buffer:STRING_BUFFER <-
  (
    buffer.append "type_id("
    receiver.display buffer
    buffer.add_last ')'
  )









