Header
  + name := MAIN;
  
Public
    
  - main <-
  ( + t:ARRAY CHARACTER
    t := ARRAY CHARACTER.create_with_capacity 6
    0.to 5 do { i:INTEGER; t.add_last ('A'+#i); t.last.print; }
    '\n'.print
    t.move_to_last 1 to 3
    STRING.tmp { tmp:STRING_BUFFER
      0.to 5 do { i:INTEGER; tmp | t.at i; }
      tmp.println
      ? {tmp == "AEFBCD"}
    }
  )
