Header
  + name := MAIN;

Public

  - main <-
  ( + a1,a2:ARRAY INTEGER
    + b1,b2:ARRAY_BLOCK CHARACTER
    //
    a1 := ARRAY INTEGER .create_with_capacity 1024
    a1.add_last 2
    a2 := a1.twin
    TEST.check (a1.upper = a2.upper)
    TEST.check (a1.first = a2.last)
    //
    b1 := ARRAY_BLOCK CHARACTER .new
    b1.add_last '2'
    b2 := b1.twin
    TEST.check (b1.upper = b2.upper)
    TEST.check (b1.first = b2.last)
    UTILS.exit 0
    ("bug " + "init_string_alias").to_string_alias
  )

