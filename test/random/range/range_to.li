Header
  + name := RANGE_TO;
 
Public
  - main <-
  ( + random: RANDOM
    + mt19937: MT19937
    + v1:INTEGER
    random := new RANDOM.init 3
    v1 := random.min 1 max 10
    TEST.check v1 integer 10
    v1 := random.min 1 max 10
    TEST.check v1 integer 3
    v1 := random.min 1 max 10
    TEST.check v1 integer 3
    v1 := random.min 1 max 10
    TEST.check v1 integer 7
    v1 := random.min 1 max 10
    TEST.check v1 integer 2
    v1 := random.min 1 max 10
    TEST.check v1 integer 7
    v1 := random.min 1 max 10
    TEST.check v1 integer 7
    v1 := random.min 1 max 10
    TEST.check v1 integer 2
    v1 := random.min 1 max 10
    TEST.check v1 integer 8    
    v1 := random.min 1 max 10
    TEST.check v1 integer 2
    v1 := random.min 1 max 10
    TEST.check v1 integer 5
    v1 := random.min 1 max 10
    TEST.check v1 integer 4
    v1 := random.min 1 max 10
    TEST.check v1 integer 2
    v1 := random.min 1 max 10
    TEST.check v1 integer 4
    v1 := random.min 1 max 10
    TEST.check v1 integer 2
    v1 := random.min 1 max 10
    TEST.check v1 integer 3
    while {v1 != 1} do {
      v1 := random.min 1 max 10
      TEST.check TRUE
    }
    TEST.check (TEST.counter) integer 25
    
    mt19937 := new MT19937.init 3
    v1 := mt19937.min 1 max 10
    TEST.check v1 integer 7
    v1 := mt19937.min 1 max 10
    TEST.check v1 integer 9
    v1 := mt19937.min 1 max 10
    TEST.check v1 integer 8
    while {v1 != 1} do {
      v1 := mt19937.min 1 max 10
      TEST.check TRUE
    }
    TEST.check (TEST.counter) integer 31
  )
